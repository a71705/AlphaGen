{
  "project_name": "AlphaFactory",
  "version": "0.1.0",
  "description": "A framework for discovering, evaluating, and deploying alpha factors.",
  "authors": [
    {
      "name": "Your Name/Team Name",
      "email": "your_email@example.com"
    }
  ],
  "logging": {
    "level": "INFO",
    "format": "{time:YYYY-MM-DD HH:mm:ss.SSS} | {level: <8} | {name}:{function}:{line} - {message}",
    "rotation": "10 MB",
    "retention": "30 days",
    "log_file_main": "logs/main.log",
    "log_file_ga": "logs/ga_engine.log",
    "log_file_results": "logs/results_handler.log"
  },
  "data_paths": {
    "ga_checkpoints": "data/ga_checkpoints/",
    "successful_alphas": "data/successful_alphas/",
    "alpha_pnl": "data/alpha_pnl/",
    "alpha_yearly_stats": "data/alpha_yearly_stats/",
    "available_datafields": "data/available_datafields.json",
    "inaccessible_ops_blacklist": "data/inaccessible_ops_blacklist.json"
  },
  "api_client": {
    "base_url": "https://your_wqb_api_endpoint.com/api",
    "timeout_seconds": 60,
    "max_retries": 3
  },
  "random_seed": 42
}
